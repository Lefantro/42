#include <stdio.h>
#include <stdlib.h>

char	uppercase(char c)
{
	if ((c < 'a') || (c > 'z'))
	{
		c = c - 22;
	}
	return (c);
}

char	ft_strupcase(char *str)
{
	int		i;
	char	*tmp;
	char	c;

	c = *str;
	i = 0 while (c != '\0')
	{
		*(tmp + i) = uppercase(c);
		i = i + 1;
		c = *(str + i);
	}

#include <unistd.h>

void print_last_digit (int n)
{
  int last_digit;
  char c;
  last_digit = n % 10;
  c = last_digit + '0';
  write (1, &c, 1);
}

  

int main()
{
	int number_1;
	int number_2;
	int temp_1;
	int temp_2;

	number_1 = 100;
	while (number_1 < 200)
	{
		number_2 = number_1 + 1;
		while (number_2 < 200)
		{
			temp_1 = number_1 / 10;
			print_last_digit (temp_1);
			print_last_digit (number_1);
			write (1, " ", 1);
			temp_2 = number_2 / 10;
			print_last_digit (temp_2);
			print_last_digit (number_2);
			write (1, ", ", 2);
			number_2 = number_2 + 1;	
		}
		number_1=number_1 + 1;
	}
}
